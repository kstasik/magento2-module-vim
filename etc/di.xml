        <arguments>
            <argument name="commands" xsi:type="array">
                <item name="generate_config" xsi:type="object">Kstasik\Vim\Console\Command\GenerateConfigCommand</item>
            </argument>
        </arguments>
    </type>

    <type name="Kstasik\Vim\Model\Config\Generator">
