                <item name="xsd" xsi:type="object">Kstasik\Vim\Model\Config\Processor\Xsd</item>
                <item name="links" xsi:type="object">Kstasik\Vim\Model\Config\Processor\Links</item>
            </argument>
        </arguments>
    </type>
</config>
